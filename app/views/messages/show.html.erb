<h1>Messages#show</h1>
<p><%= @msg %></p>
<table>
  <tr><th>ID</th>
    <td><%= @message.id %></td></tr>
  <tr><th>Person</th>
    <td><%= @message.person_id %></td></tr>
  <tr><th>Title</th>
    <td><%= @message.title %></td></tr>
  <tr><th>Message</th>
    <td><%= @message.message %></td></tr>
  <tr><th>Created</th>
    <td><%= @message.created_at %></td></tr>
  </tr>
</table>
  
