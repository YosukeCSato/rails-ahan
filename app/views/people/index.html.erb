<h1>People#index</h1>
<p><%= @msg %></p>

<pre>
<table>
<tr>
<th>Id</th><th>name</th><th>age</th><th>mail</th>
</tr>
<% @data.each do |obj| %>
 <tr>
 <td><%= obj.id %></td>
 <td><a href="/people/<%= obj.id %>"><%= obj.name %></td>
 <td><%= obj.age %></td>
 <td><%= obj.mail %></td>
 <% end %>
 </table>
</pre>
